-- MAIN RENDER LOOP
-------------------------------------------------------------------------------
RunService.RenderStepped:Connect(function()
    local color = GetRGB()
    local center = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)

    -- Crosshair & FOV
    local spin = tick() * 12
    Cursor1.Visible = Options.Crosshair.Value; Cursor1.Color = color
    Cursor1.From = center + Vector2.new(math.cos(spin)*12, math.sin(spin)*12); Cursor1.To = center + Vector2.new(math.cos(spin+math.pi)*12, math.sin(spin+math.pi)*12)
    Cursor2.Visible = Options.Crosshair.Value; Cursor2.Color = color
    Cursor2.From = center + Vector2.new(math.cos(spin+math.pi/2)*12, math.sin(spin+math.pi/2)*12); Cursor2.To = center + Vector2.new(math.cos(spin-math.pi/2)*12, math.sin(spin-math.pi/2)*12)
    SilentCircle.Visible = Options.SilentAim.Value; SilentCircle.Position = center; SilentCircle.Radius = Options.SilentFov.Value; SilentCircle.Color = color

    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LP then
            local char = p.Character
            if char and char:FindFirstChild("HumanoidRootPart") and char:FindFirstChild("Humanoid") then
                local hrp = char.HumanoidRootPart
                local hum = char.Humanoid
                local pos, onScreen = Camera:WorldToViewportPoint(hrp.Position)
                local enemy = (p.Team ~= LP.Team or p.Team == nil)
                
                -- Hitboxes (In Render Loop to prevent reset)
                if Options.SilentAim.Value and enemy then
                    for _, part in pairs(char:GetChildren()) do
                        if part:IsA("BasePart") and (part.Name == "Head" or part.Name == "FakeHead" or part.Name == "HeadHB") then
                            part.CanCollide = false
                            part.Size = Vector3.new(Options.HitboxSize.Value, Options.HitboxSize.Value, Options.HitboxSize.Value)
                            part.Transparency = Options.HitboxTrans.Value
                        end
                    end
                end

                -- ESP Logic
                if not EspStore[p] then CreateEsp(p) end
                local e = EspStore[p]
                local isVisible = onScreen and enemy

                e.Name.Visible = isVisible and Options.EspNames.Value
                e.Dist.Visible = isVisible and Options.EspDist.Value
                e.HealthBar.Visible = isVisible and Options.EspHealth.Value
                e.HealthOutline.Visible = isVisible and Options.EspHealth.Value

                if isVisible then
                    local d = math.floor((LP.Character.HumanoidRootPart.Position - hrp.Position).Magnitude)
                    e.Name.Text = p.Name; e.Name.Position = Vector2.new(pos.X, pos.Y - 45); e.Name.Color = color
                    e.Dist.Text = "[" .. d .. "m]"; e.Dist.Position = Vector2.new(pos.X, pos.Y - 30); e.Dist.Color = Color3.new(1,1,1)
                    
                    if Options.EspHealth.Value then
                        local hp = hum.Health / hum.MaxHealth
                        local barSize = 40
                        e.HealthOutline.From = Vector2.new(pos.X - 25, pos.Y - barSize/2)
                        e.HealthOutline.To = Vector2.new(pos.X - 25, pos.Y + barSize/2)
                        e.HealthOutline.Thickness = 3; e.HealthOutline.Color = Color3.new(0,0,0)
                        
                        e.HealthBar.From = Vector2.new(pos.X - 25, pos.Y + barSize/2)
                        e.HealthBar.To = Vector2.new(pos.X - 25, pos.Y + barSize/2 - (barSize * hp))
                        e.HealthBar.Thickness = 2; e.HealthBar.Color = Color3.fromHSV(hp * 0.3, 1, 1)
                    end
                end

                -- Highlight & Tracers
                local h = char:FindFirstChild("Highlight")
                if Options.Esp.Value and enemy then
                    if not h then h = Instance.new("Highlight", char) end
                    h.Enabled = true; h.FillColor = color
                elseif h then h.Enabled = false end

                if Options.Tracers.Value and enemy and onScreen then
                    if not TracersStore[p] then TracersStore[p] = Drawing.new("Line") end
                    TracersStore[p].Visible = true; TracersStore[p].From = Vector2.new(center.X, Camera.ViewportSize.Y); TracersStore[p].To = Vector2.new(pos.X, pos.Y); TracersStore[p].Color = color
                elseif TracersStore[p] then TracersStore[p].Visible = false end
            end
        end
    end
end)

